<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

    
<GridPane fx:controller="kdtrees.gui.ResultPageController"
    xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="5" vgap="10">
	<padding><Insets top="25" right="25" bottom="10" left="25"/></padding>
  	<columnConstraints>
    	<ColumnConstraints hgrow="ALWAYS" minWidth="125.0" prefWidth="125.0" maxWidth="125.0"/>
  	</columnConstraints>
  	<rowConstraints>
    	<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    	<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    	<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  	</rowConstraints>
 
    <Label text="Your Match:"
        GridPane.columnIndex="0" GridPane.rowIndex="0"/>

	<Text fx:id="resultText" text="Result"
		GridPane.columnIndex="0" GridPane.rowIndex="1" />

    <!-- TODO: Add a Text element with the id "resultText" -->

	<HBox spacing="10" alignment="center"
		GridPane.columnIndex="0" GridPane.rowIndex="2">
	<Button fx:id="back" text="Back" onAction="#handleBackButtonAction"/>
		</HBox>
    

    </GridPane>
